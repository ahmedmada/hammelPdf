<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingEnd="@dimen/pdfPaint_welcome_margin_side"
    android:paddingStart="@dimen/pdfPaint_welcome_margin_side"
    android:paddingTop="@dimen/pdfPaint_welcome_margin_top">

    <TextView
        android:id="@+id/pdfPaint_intro_started_head"
        style="@style/PdfPaintWelcomeHeader"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minLines="2"
        android:text="@string/enjoy_hammel_pdf"/>

    <TextView
        android:id="@+id/pdfPaint_intro_started_text"
        style="@style/PdfPaintWelcomeText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/pdfPaint_intro_started_head"
        android:layout_marginTop="@dimen/pdfPaint_welcome_margin_top_text"
        android:text="@string/intro_get_started"/>
</RelativeLayout>
